{
	"name": "@shockpkg/resedit",
	"description": "Repacked resedit with both esm and cjs support",
	"version": "2.0.1",
	"keywords": [
		"resedit"
	],
	"main": "cjs/index",
	"exports": {
		".": [
			{
				"import": "./esm/index.mjs",
				"require": "./cjs/index.js",
				"default": "./cjs/index.js"
			},
			"./cjs/index.js"
		]
	},
	"types": "dts/index.d.ts",
	"module": "esm/index.mjs",
	"sideEffects": false,
	"engines": {
		"node": ">=14"
	},
	"scripts": {
		"clean": "rimraf *.log* dts esm cjs",
		"build": "rollup -c",
		"all": "npm run clean && npm run build",
		"prepack": "npm run clean && npm run build"
	},
	"repository": "https://github.com/shockpkg/resedit.git",
	"bugs": "https://github.com/shockpkg/resedit/issues",
	"author": "JrMasterModelBuilder",
	"contributors": [
		"jet",
		"JrMasterModelBuilder"
	],
	"license": "MIT",
	"devDependencies": {
		"@rollup/plugin-node-resolve": "^15.2.2",
		"pe-library": "^1.0.0",
		"resedit": "^2.0.0",
		"rimraf": "^5.0.5",
		"rollup": "^3.29.4",
		"rollup-plugin-dts": "^6.0.2",
		"typescript": "^5.2.2"
	}
}
